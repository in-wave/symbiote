  },
  "homepage": "https://symbiotejs.org/",
  "repository": {
    "type": "git",
    "url": "https://github.com/symbiotejs/symbiote.js.git"
  },
  "keywords": [
    "symbiote.js, web components, custom elements, shadow dom, ui framework"
